<!--
  family-graph: Interactive canvas-based family tree visualization.
  Supports panning and tap-to-select nodes.
-->
<canvas
  type="2d"
  id="familyGraph"
  class="graph-canvas"
  style="width:{{width}}px;height:{{height}}px;"
  bindtouchstart="onTouchStart"
  bindtouchmove="onTouchMove"
  bindtouchend="onTouchEnd"
></canvas>
